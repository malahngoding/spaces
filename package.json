{
  "name": "@malahngoding/spaces",
  "version": "0.0.1",
  "description": "Malah Ngoding Spaces",
  "private": true,
  "scripts": {
    "generate": "sh proto/generate.sh",
    "dev": "next -p 3500",
    "build": "next build",
    "export": "next build && next export",
    "start": "next build && next start -p 3500",
    "test": "jest",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "npm run lint --fix",
      "npm run format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malahngoding/spaces.git"
  },
