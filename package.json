{
  "name": "@malahngoding/spaces",
  "version": "0.0.2",
  "description": "Malah Ngoding Spaces",
  "private": true,
  "scripts": {
    "generate": "sh proto/generate.sh",
    "dev": "next -p 3500",
    "build": "next build",
    "export": "next build && next export",
    "start": "next build && next start -p 3500",
    "serve": "next start -p 3500",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "npm run lint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malahngoding/spaces.git"
  },
  "author": "Malah Ngoding Bot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malahngoding/spaces/issues"
  },
  "homepage": "https://github.com/malahngoding/spaces#readme",
  "keywords": [
    "nextjs",
    "typescript"
  ],
  "dependencies": {
    "@fontsource/mononoki": "4.5.0",
    "@grpc/grpc-js": "1.4.2",
    "@grpc/proto-loader": "0.6.6",
    "react-hook-form": "7.18.1",
    "@fontsource/montserrat": "^4.5.1",
    "@iconscout/react-unicons": "^1.1.6",
    "@radix-ui/colors": "^0.1.7",
    "@radix-ui/react-avatar": "^0.1.1",
    "@radix-ui/react-popover": "^0.1.1",
    "@stitches/react": "^1.2.5",
    "next": "12.0.3",
    "next-auth": "4.0.0-beta.2",
    "next-intl": "^2.2.0",
    "next-themes": "^0.0.15",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sharp": "^0.29.2",
    "swr": "^1.0.1",
    "zustand": "^3.6.5"
  },
  "devDependencies": {
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@types/google-protobuf": "3.15.5",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "grpc-tools": "1.11.2",
    "grpc_tools_node_protoc_ts": "5.3.2",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "@types/node": "16.11.7",
    "@types/react": "17.0.34",
    "eslint": "7",
    "eslint-config-next": "12.0.3",
    "typescript": "4.4.4"
  }
}
